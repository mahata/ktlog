{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build-watch": "tsc && vite build --watch --emptyOutDir --outDir ../backend/src/main/resources/static",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix && stylelint --fix src/**/*.scss && prettier --write src/**/*.ts src/**/*.tsx src/**/*.scss",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "dependencies": {
    "jotai": "^2.6.0",
    "lucide-react": "^0.301.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "reset-css": "^5.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^0.34.6"
  }
}
